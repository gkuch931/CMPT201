#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>

int main() {
  char *line = NULL;
  size_t len = 0;
  char *saveptr;
  getline(&line, &len, stdin);
  char *token = strtok_r(line, "\n", &saveptr);
  while (1) {
    if (fork() == 0) {
      printf("command to be run: %s", token);
      execl(token, token, NULL);
    }
  }
}
