#define _POSIX_C_SOURCE 200809L
#include <stdio.h>

int main() {
  char *line = NULL;
  size_t len = 0;
  char *stringArr[5];
  int i = 0;
  while (1) {
    getline(&line, &len, stdin);
    stringArr[i] = line;
    line = NULL;
    printf("%s", *stringArr[i]);
    i++;
    i = i % 4;
  }
  printf("%s", line);
}
