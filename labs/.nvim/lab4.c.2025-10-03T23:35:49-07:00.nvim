#include <stdint.h>
#include <stdio.h>
#include <unistd.h>

struct header {
  uint64_t size;
  struct header *next;
};

int main() {
  intptr_t *program_break = sbrk(256);
  struct header *header1 = (struct header *)program_break;
  printf("%lu\n", sizeof(intptr_t));
  printf("%lu\n", sizeof(uint64_t));
  

  struct header *header2 = (struct header *)(program_break[16]);
  printf("%i\n", &header1);
  printf("%i\n", &header2);
}
